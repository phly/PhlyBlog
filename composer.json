{
    "name": "phly/phly-blog",
    "type": "library",
    "description": "Laminas Module implementing a static blog generator",
    "keywords": [
        "laminas",
        "zf2",
        "zend",
        "module",
        "blog"
    ],
    "homepage": "https://github.com/phly/PhlyBlog",
    "require": {
        "php": "^7.3",
        "laminas/laminas-dependency-plugin": "^1.0",
        "laminas/laminas-eventmanager": "^2.0",
        "laminas/laminas-feed": "^2.0",
        "laminas/laminas-filter": "^2.0",
        "laminas/laminas-i18n": "^2.0",
        "laminas/laminas-inputfilter": "^2.0",
        "laminas/laminas-modulemanager": "^2.0",
        "laminas/laminas-mvc": "^2.0",
        "laminas/laminas-paginator": "^2.0",
        "laminas/laminas-tag": "^2.0",
        "laminas/laminas-uri": "^2.0",
        "laminas/laminas-validator": "^2.0",
        "laminas/laminas-view": "^2.0",
        "phly/phly-common": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "ext-json": "*"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-0": {
            "PhlyBlog": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhlyBlogTest\\": "test/PhlyBlog/"
        }
    }
}
